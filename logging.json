{
    "version": 1,
    "disable_existing_loggers": false,
    "objects": {},
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(threadName)s - %(name)s - %(levelname)s - %(message)s"
        },
        "colored": {
            "()": "supportlogging.ColoredFormatter",
            "format": "%(asctime)s.%(msecs)03d - %(threadName)-15s - %(name)-15s - %(levelname)-17s - %(message)s",
            "datefmt": "%H:%M:%S"
        },
        "file_format": {
            "()": "supportlogging.FileFormatter",
            "format": "%(asctime)s - %(threadName)-15s - %(name)-15s - %(levelname)-17s - %(message)s"
        }
    },
    "filters": {
        "filtermodules": {
            "()": "supportlogging.FilterModule",
            "patterns": [
                "urllib3.connectionpool",
                "httpx",
                "PIL.TiffImagePlugin",
                "img2pdf",
                "hpack",
                "remote_connection",
                "passlib",
                "backoff",
                "googleapiclient.discovery_cache",
                "charset_normalizer",
                "blib2to3.pgen2.driver",
                "pyrate_limiter.limit_context_decorator",
                "oauth2client.client",
                "parso.python.diff",
                "httpcore",
                "mitmproxy",
                "seleniumwire.handle",
                "seleniumwire.server"
            ]
        },
        "filtermsg": {
            "()": "supportlogging.FilterMsg",
            "patterns": [
                {
                    "name": "proxy.common.plugins",
                    "text": [
                        "Loaded plugin"
                    ]
                },
                {
                    "name": "proxy.core.listener.tcp",
                    "text": [
                        "Listening on"
                    ]
                },
                {
                    "name": "proxy.core.acceptor.pool",
                    "text": [
                        "Started",
                        "Shutting down"
                    ]
                },
                {
                    "name": "yt_dlp",
                    "text": [
                        "Sort order given by user",
                        "Formats sorted by",
                        "Downloading item",
                        "Downloading playlist",
                        "Finished downloading playlist"
                    ]
                }
            ]
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "colored",
            "filters": [
                "filtermodules",
                "filtermsg"
            ],
            "stream": "ext://sys.stdout"
        },
        "info_file_handler": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "file_format",
            "filename": "{path_logs}/asyncdl.log",
            "filters": [
                "filtermodules",
                "filtermsg"
            ],
            "maxBytes": 3155360,
            "backupCount": 20,
            "encoding": "utf8"
        },
        "queue_handler": {
            "class": "supportlogging.QueueListenerHandler",
            "handlers": ["cfg://handlers.console", "cfg://handlers.info_file_handler"],
            "respect_handler_level": "True"
        }
    },
    "loggers": {},
    "root": {
        "level": "DEBUG",
        "handlers": ["queue_handler"]
    }
}